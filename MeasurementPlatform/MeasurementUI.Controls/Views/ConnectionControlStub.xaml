<UserControl x:Class="MeasurementUI.Controls.Views.ConnectionControlStub"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MeasurementUI.Controls.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             >
    <!--DataContext="{Binding Path=ConnectionControlStubViewModel, Source={StaticResource ViewModelLocator}}"-->
    <StackPanel>
        <TextBlock Text="Connection Control Stub" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="30"/>
        <Button x:Name="CloseButton" Content="Close" Click="CloseButton_Click"/>
        <TextBox x:Name="TestNum" Text="{Binding TestNum}" Width="100"/>
    </StackPanel>
</UserControl>
